{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-ms-touch-action"
        ]
      }
    ]
  }
}